{
    "presets": [
        "react",
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                },
                "useBuiltIns": true
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "transform-object-rest-spread",
        "react-html-attrs",
        "syntax-dynamic-import",
        "transform-remove-console",
        "transform-object-assign"
    ]
}